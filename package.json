{
  "name": "llamune_code",
  "version": "0.1.0",
  "description": "Llamune のコーディング支援特化版 - リポジトリツールで LLM がコードベースにアクセス",
  "main": "dist/index.js",
  "bin": {
    "llamune": "./bin/llamune.js",
    "llmn": "./bin/llmn.js"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "setup": "node scripts/generate-secrets.js",
    "encrypt-domain-prompts": "tsx scripts/encrypt-domain-prompts.ts",
    "api": "tsx src/api/server.ts 2>&1 | tee -a api-debug.log",
    "api:build": "tsc && node dist/api/server.js 2>&1 | tee -a api-debug.log",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "check-db": "tsx scripts/check-db.ts",
    "check-encryption": "tsx scripts/check-encryption.ts",
    "test-auth": "tsx scripts/test-auth-migration.ts",
    "update-models": "tsx scripts/update-recommended-models.ts",
    "cleanup-llamune-db": "tsx scripts/cleanup-llamune-db.ts",
    "cleanup-admin-chats": "tsx scripts/cleanup-admin-chats.ts",
    "check-sessions": "tsx scripts/check-sessions.ts",
    "delete-sessions": "tsx scripts/delete-sessions.ts",
    "create-user": "tsx scripts/create-test-user.ts",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "migrate:status": "knex migrate:status --knexfile knexfile.ts"
  },
  "keywords": [
    "llm",
    "ollama",
    "cli",
    "local",
    "ai"
  ],
  "author": "mop",
  "license": "MIT",
  "engines": {
    "node": "22.21.0"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^9.2.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "ink": "^4.4.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "react": "^18.2.0",
    "systeminformation": "^5.27.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/knex": "^0.15.2",
    "@types/node": "^20.19.25",
    "@types/react": "^18.2.46",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
