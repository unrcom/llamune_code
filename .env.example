# Llamune API Server Configuration

# API Server Port
PORT=3000

# JWT Secret (256-bit random string)
# Generate with: openssl rand -base64 32
# または: node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
JWT_SECRET=your-256-bit-secret-here-change-this

# JWT Token Expiry
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d

# Ollama API URL
OLLAMA_API_URL=http://localhost:11434
